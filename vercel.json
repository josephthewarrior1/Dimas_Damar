{
    "version": 2,
    "rewrites": [
      { "source": "/assets/(.*)", "destination": "/assets/$1" },
      { "source": "/(.*)", "destination": "/index.html" }
    ],
    "cleanUrls": true,
    "trailingSlash": false,
    "routes": [
      {
        "src": "/.*",
        "dest": "/index.html",
        "status": 200
      }
    ]
  }